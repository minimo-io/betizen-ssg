{
    "redirects": [
        {
            "source": "/es/:path*",
            "destination": "/:path*",
            "permanent": true
        },
        {
            "source": "/:path*",
            "destination": "/en/:path*",
            "locale": "en",
            "status": 302
        },
        {
            "source": "/:path*",
            "destination": "/pt-br/:path*",
            "locale": "pt-br",
            "status": 302
        }
    ]
}
