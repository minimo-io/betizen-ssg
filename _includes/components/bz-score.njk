<div class="rating-display theme-purchases__item__inner text-center pt-0 {% if paddingBottom %}{{paddingBottom}}{% endif %}">
    
    {% for i in range(1, 6) %}
        <span
            class="fa fa-star {% if i <= score %}checked{% endif %}"
            style="{{ starSize }}"
        ></span>
    {% endfor %}

</div>
