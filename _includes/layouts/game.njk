                        class="d-flex justify-content-between align-items-center mb-2"
                    >
                        <a
                            class="link--dark ranking-dropdown"
                            js-price-dropdown="true"
                            href="/rankings/"
                            data-toggle="dropdown"
                            aria-haspopup="true"
                            aria-expanded="false"
