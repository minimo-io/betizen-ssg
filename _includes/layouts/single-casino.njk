---
layout: layouts/base.njk
---

{%- from "macros/reputation-button.njk" import reputationButton with context -%}
{%- include "components/casinos/casino-header.njk" -%}
{%- set logoPath -%}{{ metadata.images.casinos }}{{ page.fileSlug }}-logo.webp{%- endset -%}

<section class="section pt-0">
  <div class="container">
    <div class="row">
      <div id="container">
        <div id="content" role="main">
          <div class="row">
            <div class="col-lg-8 mb-md-0 mb-3">
              <div
                id="casino-110"
                class="product type-product status-publish has-post-thumbnail product_cat-landing-corporate first casino-single"
              >
                <div class="row box-did-u-know">
                  <div class="container-fluid">
                    <div class="card border-0">
                      <div class="card-body pb-0 px-0">
                        <h2 class="card-title ml-1">
                          Reseña del casino {{ title }}
                        </h2>

                        {%- include "components/casinos/casino-bonus.njk" -%}

                        {%- include "components/casinos/casino-details.njk" -%}
                        
                        {%- include "components/casinos/casino-featured.njk" -%}

                        <br />
                        <p class="mb-3">
                          {{ excerpt }}
                        </p>
                      </div>
                    </div>
                  </div>
                </div>
            
                {%- include "components/casinos/casino-gallery.njk" -%}

                <div class="card-text general-description Xminimo-read-more Xminimo-read-more-short px-1">
                  <br />
                  {{ content | safe }}
                </div>

                {%- include "components/casinos/casino-providers.njk" -%}

                <div class="d-lg-none">
                  <div
                    class="d-flex justify-content-between align-items-center mb-3"
                  >
                    <div>
                      <h4 class="mt-3 mb-1">Vera&amp;John</h4>
                      Reputación&nbsp;
                      <span class="casino-reputation-box">
                        {{ reputationButton(reputation.code, reputation.text, logoPath, title, gradient, bonus.link, true) }}
                      </span>
                    </div>
                    <h3
                      class="d-flex align-items-center"
                      js-price-value="main_price_div"
                    >
                      <span
                        class="woocommerce-Price-amount amount ranking-small mr-1"
                        ><span class="woocommerce-Price-currencySymbol">#</span
                        >{{ ranking }}</span
                      ><a href="{{ "/rankings/" | locale_url }}"
                        ><sup
                          ><i
                            class="fa fa-info-circle"
                            aria-hidden="true"
                          ></i></sup
                      ></a>
                    </h3>
                  </div>
                  <div
                    class="d-flex justify-content-between align-items-center mb-3"
                  >
                    <form action="" method="POST" class="d-block w-100">
                      <a
                        class="btn btn-brand btn-block up btn-customcolor"
                        rel="nofollow noopener"
                        href="{{ bonus.link }}"
                      >
                        <span class="btn-text">Visitar</span></a
                      >
                    </form>
                  </div>

                  <div class="theme-purchases">
                    <div class="theme-purchases__item">
                      <a
                        class="theme-purchases__item__inner text-center"
                        data-toggle="tab"
                        href="#reviews-tab"
                        role="tab"
                        js-handle="review-toggler"
                      >
                        <div
                          class="rating-display"
                          data-toggle="tooltip"
                          data-placement="top"
                          title="4"
                        >
                          <span
                            class="fa fa-star checked"
                            style="font-size: 35px"
                          ></span
                          ><span
                            class="fa fa-star checked"
                            style="font-size: 35px"
                          ></span
                          ><span
                            class="fa fa-star checked"
                            style="font-size: 35px"
                          ></span
                          ><span
                            class="fa fa-star checked"
                            style="font-size: 35px"
                          ></span
                          ><span
                            class="fa fa-star"
                            style="font-size: 35px"
                          ></span>
                        </div>
                        {# <span class="avg-inline">1 opiniones</span> #}
                      </a>
                    </div>
                  </div>
                  <div class="theme-description__list d-lg-none mb-4">
                    {# <div class="theme-description__list__item">
                      <span class="theme-description__item__title">Fundado</span
                      ><span>2011</span>
                    </div>
                    <div class="theme-description__list__item">
                      <span class="theme-description__item__title"
                        >Licencia(s)</span
                      ><span>Malta, UKGC, Dinamarca</span>
                    </div>
                    <div class="theme-description__list__item">
                      <span class="theme-description__item__title"
                        >Tiempos de retiro</span
                      ><span
                        >Billetera electrónica: Instantánea<br />
                        Credito/Débito: 3-5 días<br />
                        Transferencia: 0-3 días</span
                      >
                    </div> #}
                    <div
                      class="theme-description__list__item align-items-center"
                    >
                      <span class="theme-description__item__title"
                        >¿Preguntas?</span
                      ><a
                        class="btn btn-xs btn-outline-brand btn-customcolor-outline"
                        href="https://www.instagram.com/betizen_org/"
                        >Contáctanos</a
                      >
                    </div>
                    <div class="mt-2 text-center">
                      <a href="{{ languages[page.lang].promo.url }}" target="_blank" rel="nofollow noopener">
                          <img alt="banner-250x250" src="{{ languages[page.lang].promo.external_250x250 | safe }}" />
                      </a>
                    </div>
                  </div>
                </div>
                <div
                  class="d-flex justify-content-between align-items-center has-border"
                >
                  <ul class="nav sub-nav sub-nav--has-border" role="tablist">
                    <li class="nav-item">
                      <a
                        class="nav-link sub-nav-link active"
                        data-toggle="tab"
                        href="#promotions-tab"
                        role="tab"
                      >
                        Promociones <span class="badge badge-secondary">1</span>
                      </a>
                    </li>
                    <li class="nav-item" style="visibility: hidden">
                      <a
                        class="nav-link sub-nav-link"
                        data-toggle="tab"
                        href="#reviews-tab"
                        role="tab"
                      >
                        Reseñas&nbsp;
                        <span class="badge badge-secondary"> 1 </span>
                      </a>
                    </li>
                  </ul>
                </div>
                <div class="tab-content">
                  <div
                    class="tab-pane fade show active mt-2 mt-lg-4"
                    id="promotions-tab"
                    role="tabpanel"
                  >
                    <div class="theme-description">
                      <div class="container">
                        <div class="row">
                          <div class="col-sm-4 m-0 p-1">
                            <div class="card card-multibonus mt-2">
                              <div>
                                <img
                                  src="https://i0.wp.com/www.betizen.org/wp-content/uploads/2020/05/bonus-casino-verajohn.png?resize=400%2C246&amp;ssl=1"
                                  class="card-img-top"
                                  alt="imagen-de-bono"
                                />
                                <a
                                  href="../../bonus/bono-de-bienvenida-en-verajohn/index.html"
                                  data-toggle="tooltip"
                                  data-placement="top"
                                  title="Más información"
                                  class="card-bonus-moreinfo knowmore-icon-link"
                                  ><sup
                                    ><i
                                      class="fa fa-info-circle"
                                      aria-hidden="true"
                                    ></i></sup
                                ></a>
                              </div>
                              <div class="card-body">
                                <h5 class="card-title">
                                  Bono de bienvenida en Vera&amp;John
                                </h5>
                                <p class="card-text">
                                  100% hasta $100, x40&nbsp;<span
                                    class="badge badge-success"
                                    >activo</span
                                  >
                                </p>
                                <a
                                  rel="nofollow"
                                  target="_blank"
                                  role="button"
                                  href="../../visit/verajohnes/index.html"
                                  class="btn btn-bonus btn-outline-brand btn-block mb-1 ml-0 btn-customcolor-outline text-uppercase"
                                >
                                  <i
                                    class="fa fa-thumbs-up"
                                    aria-hidden="true"
                                  ></i>
                                  Obtener bono
                                </a>
                              </div>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <div
              class="col-lg-4 d-none d-lg-block pl-xs-0 pl-lg-5 casino-sidebar"
            >
              <div class="sidebar-lg">
                <div class="theme-purchases">
                  <div
                    class="text-center Xd-flex Xjustify-content-between align-items-center mb-1"
                  >
                    <h2 class="Xd-flex align-items-center">
                      <span class="woocommerce-Price-amount amount ranking-big"
                        ><span class="woocommerce-Price-currencySymbol"></span
                        >#{{ ranking }}</span
                      ><a
                        class="knowmore-icon-main"
                        href="{{ "/rankings/" | locale_url }}"
                        ><i class="fa fa-info-circle" aria-hidden="true"></i
                      ></a>
                    </h2>
                  </div>
                  <div class="theme-purchases__item" style="border-top: 0">
                    <a
                      class="theme-purchases__item__inner text-center pt-0"
                      data-toggle="tab"
                      href="#reviews-tab"
                      role="tab"
                      js-handle="review-toggler"
                      aria-expanded="true"
                    >
                      <div
                        class="rating-display"
                        data-toggle="tooltip"
                        data-placement="top"
                        title="4"
                      >
                        <span
                          class="fa fa-star checked"
                          style="font-size: 35px"
                        ></span
                        ><span
                          class="fa fa-star checked"
                          style="font-size: 35px"
                        ></span
                        ><span
                          class="fa fa-star checked"
                          style="font-size: 35px"
                        ></span
                        ><span
                          class="fa fa-star checked"
                          style="font-size: 35px"
                        ></span
                        ><span
                          class="fa fa-star"
                          style="font-size: 35px"
                        ></span>
